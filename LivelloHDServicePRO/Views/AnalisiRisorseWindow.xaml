<Window x:Class="LivelloHDServicePRO.Views.AnalisiRisorseWindow"
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
xmlns:local="clr-namespace:LivelloHDServicePRO.Views"
mc:Ignorable="d"
Title="Analisi Dettagliata Risorse" Height="800" Width="1200"
WindowStartupLocation="CenterOwner" WindowState="Maximized" Background="WhiteSmoke">

    <Window.Resources>
        <Style x:Key="CardStyle" TargetType="Border">
            <Setter Property="Background" Value="White"/>
            <Setter Property="CornerRadius" Value="8"/>
            <Setter Property="Padding" Value="15"/>
            <Setter Property="Margin" Value="10"/>
            <Setter Property="Effect">
                <Setter.Value>
                    <DropShadowEffect Color="Gray" Opacity="0.3" ShadowDepth="3" BlurRadius="8"/>
                </Setter.Value>
            </Setter>
        </Style>

        <Style x:Key="SectionTitleStyle" TargetType="TextBlock">
            <Setter Property="FontSize" Value="16"/>
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="Foreground" Value="#FF9C27B0"/>
            <Setter Property="Margin" Value="0,0,0,10"/>
        </Style>

        <Style x:Key="LabelStyle" TargetType="TextBlock">
            <Setter Property="FontWeight" Value="Bold"/>
            <Setter Property="Margin" Value="0,5,10,5"/>
        </Style>

        <Style x:Key="ValueStyle" TargetType="TextBlock">
            <Setter Property="Margin" Value="0,5"/>
        </Style>
    </Window.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>

        <!-- Header -->
        <Border Grid.Row="0" Background="#FF9C27B0" Padding="20">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="Auto"/>
                </Grid.ColumnDefinitions>

                <StackPanel Grid.Column="0">
                    <TextBlock Text="ANALISI DETTAGLIATA RISORSE" 
                               FontSize="24" FontWeight="Bold" Foreground="White"/>
                    <TextBlock x:Name="RisorseCountText" Text="Risorse analizzate: 0" 
                               FontSize="14" Foreground="#F3E5F5" Margin="0,5,0,0"/>
                </StackPanel>

                <Button Grid.Column="1" Content="Chiudi" Click="ChiudiButton_Click"
                        Background="White" Foreground="#FF9C27B0" 
                        Padding="15,8" FontWeight="Bold"/>
            </Grid>
        </Border>

        <!-- Contenuto -->
        <Grid Grid.Row="1" Margin="10">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="300"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>

            <!-- Lista Risorse -->
            <Border Grid.Column="0" Style="{StaticResource CardStyle}" Margin="0,0,5,0">
                <StackPanel>
                    <TextBlock Text="SELEZIONA RISORSA" Style="{StaticResource SectionTitleStyle}"/>
                    <ListBox x:Name="RisorseListBox" 
                             SelectionChanged="RisorseListBox_SelectionChanged"
                             DisplayMemberPath="NomeProprietario"
                             Height="Auto" MaxHeight="650"/>
                </StackPanel>
            </Border>

            <!-- Dettaglio Risorsa -->
            <ScrollViewer Grid.Column="1" VerticalScrollBarVisibility="Auto">
                <StackPanel x:Name="DettaglioPanel" Margin="5,0,0,0">
                    
                    <!-- Card Statistiche Principali -->
                    <Border Style="{StaticResource CardStyle}">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>

                            <StackPanel Grid.Column="0" HorizontalAlignment="Center">
                                <TextBlock x:Name="VotazioneText" Text="100/100" 
                                          FontSize="48" FontWeight="Bold" 
                                          HorizontalAlignment="Center" Foreground="#FF9C27B0"/>
                                <TextBlock Text="VALUTAZIONE TOTALE" FontSize="12" 
                                          HorizontalAlignment="Center" Foreground="Gray" Margin="0,0,0,5"/>
                            </StackPanel>

                            <StackPanel Grid.Column="1" HorizontalAlignment="Center">
                                <TextBlock x:Name="TicketTotaliText" Text="0" 
                                          FontSize="36" FontWeight="Bold" 
                                          HorizontalAlignment="Center" Foreground="#FF2196F3"/>
                                <TextBlock Text="TICKET GESTITI" FontSize="12" 
                                          HorizontalAlignment="Center" Foreground="Gray"/>
                            </StackPanel>

                            <StackPanel Grid.Column="2" HorizontalAlignment="Center">
                                <TextBlock x:Name="PercentualeRisoluzioneText" Text="0%" 
                                          FontSize="36" FontWeight="Bold" 
                                          HorizontalAlignment="Center" Foreground="#4CAF50"/>
                                <TextBlock Text="% RISOLUZIONE" FontSize="12" 
                                          HorizontalAlignment="Center" Foreground="Gray"/>
                            </StackPanel>
                        </Grid>
                    </Border>
                    
                    <!-- Dettaglio Punteggi per Categoria -->
                    <Border Style="{StaticResource CardStyle}">
                        <StackPanel>
                            <TextBlock Text="DETTAGLIO PUNTEGGI" Style="{StaticResource SectionTitleStyle}"/>
                            <Grid>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                
                                <!-- Colonna Sinistra -->
                                <StackPanel Grid.Column="0" Margin="0,0,10,0">
                                    <!-- TMC rispetto a SLA -->
                                    <Border Background="#E3F2FD" CornerRadius="4" Padding="10,8" Margin="0,0,0,8">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0" Text="TMC rispetto a SLA" FontSize="11" FontWeight="SemiBold" 
                                                       Foreground="#1976D2" VerticalAlignment="Center"/>
                                            <TextBlock Grid.Column="1" x:Name="PunteggioTMCText" Text="20/20" 
                                                       FontSize="14" FontWeight="Bold" Foreground="#1976D2" VerticalAlignment="Center"/>
                                        </Grid>
                                    </Border>
                                    
                                    <!-- T-EFF rispetto a SLA -->
                                    <Border Background="#E8F5E9" CornerRadius="4" Padding="10,8" Margin="0,0,0,8">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0" Text="T-EFF rispetto a SLA" FontSize="11" FontWeight="SemiBold" 
                                                       Foreground="#388E3C" VerticalAlignment="Center"/>
                                            <TextBlock Grid.Column="1" x:Name="PunteggioTEFFText" Text="20/20" 
                                                       FontSize="14" FontWeight="Bold" Foreground="#388E3C" VerticalAlignment="Center"/>
                                        </Grid>
                                    </Border>
                                    
                                    <!-- Valutazione Tempi Medi -->
                                    <Border Background="#FFF3E0" CornerRadius="4" Padding="10,8">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0" Text="Valutazione Tempi Medi" FontSize="11" FontWeight="SemiBold" 
                                                       Foreground="#F57C00" VerticalAlignment="Center"/>
                                            <TextBlock Grid.Column="1" x:Name="PunteggioTempiMediText" Text="20/20" 
                                                       FontSize="14" FontWeight="Bold" Foreground="#F57C00" VerticalAlignment="Center"/>
                                        </Grid>
                                    </Border>
                                </StackPanel>
                                
                                <!-- Colonna Destra -->
                                <StackPanel Grid.Column="1" Margin="10,0,0,0">
                                    <!-- Tasso Risoluzione -->
                                    <Border Background="#F3E5F5" CornerRadius="4" Padding="10,8" Margin="0,0,0,8">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0" Text="Tasso Risoluzione" FontSize="11" FontWeight="SemiBold" 
                                                       Foreground="#7B1FA2" VerticalAlignment="Center"/>
                                            <TextBlock Grid.Column="1" x:Name="PunteggioRisoluzioneText" Text="20/20" 
                                                       FontSize="14" FontWeight="Bold" Foreground="#7B1FA2" VerticalAlignment="Center"/>
                                        </Grid>
                                    </Border>
                                    
                                    <!-- Volume Ticket Gestiti -->
                                    <Border Background="#FCE4EC" CornerRadius="4" Padding="10,8" Margin="0,0,0,8">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0" Text="Volume Ticket Gestiti" FontSize="11" FontWeight="SemiBold" 
                                                       Foreground="#C2185B" VerticalAlignment="Center"/>
                                            <TextBlock Grid.Column="1" x:Name="PunteggioVolumeText" Text="20/20" 
                                                       FontSize="14" FontWeight="Bold" Foreground="#C2185B" VerticalAlignment="Center"/>
                                        </Grid>
                                    </Border>
                                    
                                    <!-- Totale Riassuntivo -->
                                    <Border Background="#EDE7F6" CornerRadius="4" Padding="10,8" BorderBrush="#9C27B0" BorderThickness="2">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="*"/>
                                                <ColumnDefinition Width="Auto"/>
                                            </Grid.ColumnDefinitions>
                                            <TextBlock Grid.Column="0" Text="TOTALE" FontSize="12" FontWeight="Bold" 
                                                       Foreground="#6A1B9A" VerticalAlignment="Center"/>
                                            <TextBlock Grid.Column="1" x:Name="PunteggioTotaleRiepilogoText" Text="100/100" 
                                                       FontSize="16" FontWeight="Bold" Foreground="#6A1B9A" VerticalAlignment="Center"/>
                                        </Grid>
                                    </Border>
                                </StackPanel>
                            </Grid>
                        </StackPanel>
                    </Border>

                    <!-- Sintesi Analitica -->
                    <Border Style="{StaticResource CardStyle}">
                        <StackPanel>
                            <TextBlock Text="SINTESI ANALITICA" Style="{StaticResource SectionTitleStyle}"/>
                            <TextBlock x:Name="SintesiText" Text="Seleziona una risorsa..." 
                                      TextWrapping="Wrap" FontSize="14" Foreground="#424242"/>
                        </StackPanel>
                    </Border>

                    <!-- Tempi Medi e Performance SLA -->
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <!-- Tempi Medi -->
                        <Border Grid.Column="0" Style="{StaticResource CardStyle}">
                            <StackPanel>
                                <TextBlock Text="TEMPI MEDI" Style="{StaticResource SectionTitleStyle}"/>
                                <StackPanel Orientation="Horizontal" Margin="0,5">
                                    <TextBlock Text="TMC:" Style="{StaticResource LabelStyle}"/>
                                    <TextBlock x:Name="TmcText" Text="N/A" Style="{StaticResource ValueStyle}" Foreground="#FF2196F3"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,5">
                                    <TextBlock Text="T-EFF:" Style="{StaticResource LabelStyle}"/>
                                    <TextBlock x:Name="TeffText" Text="N/A" Style="{StaticResource ValueStyle}" Foreground="#FF9800"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,5">
                                    <TextBlock Text="Deviazione TMC:" Style="{StaticResource LabelStyle}"/>
                                    <TextBlock x:Name="DeviazioneTmcText" Text="N/A" Style="{StaticResource ValueStyle}"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,5">
                                    <TextBlock Text="Deviazione T-EFF:" Style="{StaticResource LabelStyle}"/>
                                    <TextBlock x:Name="DeviazioneTeffText" Text="N/A" Style="{StaticResource ValueStyle}"/>
                                </StackPanel>
                            </StackPanel>
                        </Border>

                        <!-- Performance SLA -->
                        <Border Grid.Column="1" Style="{StaticResource CardStyle}">
                            <StackPanel>
                                <TextBlock Text="PERFORMANCE SLA" Style="{StaticResource SectionTitleStyle}"/>
                                <StackPanel Orientation="Horizontal" Margin="0,5">
                                    <TextBlock Text="Entro SLA:" Style="{StaticResource LabelStyle}"/>
                                    <TextBlock x:Name="EntroSlaText" Text="0 (0%)" Style="{StaticResource ValueStyle}" Foreground="#4CAF50"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,5">
                                    <TextBlock Text="Fuori SLA:" Style="{StaticResource LabelStyle}"/>
                                    <TextBlock x:Name="FuoriSlaText" Text="0 (0%)" Style="{StaticResource ValueStyle}" Foreground="#F44336"/>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Margin="0,5">
                                    <TextBlock Text="Posizione Team:" Style="{StaticResource LabelStyle}"/>
                                    <TextBlock x:Name="PosizioneText" Text="N/A" Style="{StaticResource ValueStyle}" FontWeight="Bold"/>
                                </StackPanel>
                            </StackPanel>
                        </Border>
                    </Grid>

                    <!-- Punti di Forza -->
                    <Border Style="{StaticResource CardStyle}">
                        <StackPanel>
                            <TextBlock Text="PUNTI DI FORZA" Style="{StaticResource SectionTitleStyle}"/>
                            <ItemsControl x:Name="PuntiDiForzaList">
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal" Margin="0,3">
                                            <TextBlock Text="?" Foreground="#4CAF50" FontWeight="Bold" Margin="0,0,10,0" FontSize="16"/>
                                            <TextBlock Text="{Binding}" TextWrapping="Wrap" Foreground="#424242"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </ItemsControl>
                            <TextBlock x:Name="NoPuntiDiForzaText" Text="Nessun punto di forza rilevato." 
                                      Foreground="Gray" Visibility="Collapsed"/>
                        </StackPanel>
                    </Border>

                    <!-- Aree di Miglioramento -->
                    <Border Style="{StaticResource CardStyle}">
                        <StackPanel>
                            <TextBlock Text="AREE DI MIGLIORAMENTO" Style="{StaticResource SectionTitleStyle}"/>
                            <ItemsControl x:Name="AreeDiMiglioramentoList">
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal" Margin="0,3">
                                            <TextBlock Text="?" Foreground="#FF9800" FontWeight="Bold" Margin="0,0,10,0" FontSize="16"/>
                                            <TextBlock Text="{Binding}" TextWrapping="Wrap" Foreground="#424242"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </ItemsControl>
                            <TextBlock x:Name="NoAreeMiglioramentoText" Text="Nessuna area di miglioramento rilevata." 
                                      Foreground="Gray" Visibility="Collapsed"/>
                        </StackPanel>
                    </Border>

                    <!-- Suggerimenti per Azioni -->
                    <Border Style="{StaticResource CardStyle}">
                        <StackPanel>
                            <TextBlock Text="SUGGERIMENTI PER AZIONI" Style="{StaticResource SectionTitleStyle}"/>
                            <ItemsControl x:Name="SuggerimentiAzioniList">
                                <ItemsControl.ItemTemplate>
                                    <DataTemplate>
                                        <StackPanel Orientation="Horizontal" Margin="0,3">
                                            <TextBlock Text="?" Foreground="#FF2196F3" FontWeight="Bold" Margin="0,0,10,0" FontSize="16"/>
                                            <TextBlock Text="{Binding}" TextWrapping="Wrap" Foreground="#424242"/>
                                        </StackPanel>
                                    </DataTemplate>
                                </ItemsControl.ItemTemplate>
                            </ItemsControl>
                            <TextBlock x:Name="NoSuggerimentiText" Text="Nessun suggerimento disponibile." 
                                       Foreground="Gray" Visibility="Collapsed"/>
                        </StackPanel>
                    </Border>

                    <!-- Ticket Fuori SLA -->
                    <Border Style="{StaticResource CardStyle}">
                        <StackPanel>
                            <TextBlock Text="TICKET FUORI SLA" Style="{StaticResource SectionTitleStyle}"/>
                            <TextBlock x:Name="TicketFuoriSlaCount" Text="Totale: 0" 
                                       FontSize="11" Foreground="#F44336" FontWeight="SemiBold" Margin="0,0,0,8"/>
                            <DataGrid x:Name="TicketFuoriSlaDataGrid" AutoGenerateColumns="False" 
                                      HeadersVisibility="Column" GridLinesVisibility="Horizontal"
                                      Background="Transparent" BorderThickness="0"
                                      CanUserAddRows="False" CanUserDeleteRows="False" 
                                      IsReadOnly="True" SelectionMode="Single" MaxHeight="300">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Numero Caso" Binding="{Binding NumeroCaso}" Width="150"/>
                                    <DataGridTextColumn Header="TMC" Binding="{Binding TMCFuoriSLA}" Width="150"/>
                                    <DataGridTextColumn Header="T-EFF" Binding="{Binding TEFFFuoriSLA}" Width="150"/>
                                    <DataGridTextColumn Header="Priorita" Binding="{Binding Priorita}" Width="100"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </StackPanel>
                    </Border>

                    <!-- Distribuzione Priorita -->
                    <Border Style="{StaticResource CardStyle}">
                        <StackPanel>
                            <TextBlock Text="DISTRIBUZIONE PER PRIORITA" Style="{StaticResource SectionTitleStyle}"/>
                            <DataGrid x:Name="PrioritaDataGrid" AutoGenerateColumns="False" 
                                      HeadersVisibility="Column" GridLinesVisibility="Horizontal"
                                      Background="Transparent" BorderThickness="0"
                                      CanUserAddRows="False" CanUserDeleteRows="False" 
                                      IsReadOnly="True" SelectionMode="Single" MaxHeight="200">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Priorita" Binding="{Binding Key}" Width="*"/>
                                    <DataGridTextColumn Header="Numero Ticket" Binding="{Binding Value}" Width="*"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </StackPanel>
                    </Border>

                    <!-- Tendenze Mensili -->
                    <Border Style="{StaticResource CardStyle}">
                        <StackPanel>
                            <TextBlock Text="TENDENZE MENSILI" Style="{StaticResource SectionTitleStyle}"/>
                            <DataGrid x:Name="TendenzeDataGrid" AutoGenerateColumns="False" 
                                      HeadersVisibility="Column" GridLinesVisibility="Horizontal"
                                      Background="Transparent" BorderThickness="0"
                                      CanUserAddRows="False" CanUserDeleteRows="False" 
                                      IsReadOnly="True" SelectionMode="Single" MaxHeight="250">
                                <DataGrid.Columns>
                                    <DataGridTextColumn Header="Mese" Binding="{Binding MeseDisplay}" Width="*"/>
                                    <DataGridTextColumn Header="Ticket" Binding="{Binding TicketsGestiti}" Width="80"/>
                                    <DataGridTextColumn Header="% Risoluzione" Binding="{Binding PercentualeRisoluzione, StringFormat={}{0:F1}%}" Width="110"/>
                                    <DataGridTextColumn Header="Tendenza" Binding="{Binding TendenzaDescrizione}" Width="120"/>
                                </DataGrid.Columns>
                            </DataGrid>
                        </StackPanel>
                    </Border>

                </StackPanel>
            </ScrollViewer>
        </Grid>

        <!-- Status Bar -->
        <StatusBar Grid.Row="2" Background="#EEEEEE">
            <StatusBarItem>
                <TextBlock x:Name="StatusText" Text="Seleziona una risorsa per visualizzare i dettagli"/>
            </StatusBarItem>
            <StatusBarItem HorizontalAlignment="Right">
                <StackPanel Orientation="Horizontal">
                    <Button x:Name="EsportaPdfButton" Content="Esporta PDF" 
                            Width="120" Height="30" Margin="5"
                            Background="#FF9C27B0" Foreground="White"
                            BorderBrush="#FF9C27B0" BorderThickness="1"
                            Click="EsportaPdfButton_Click" Cursor="Hand">
                        <Button.Style>
                            <Style TargetType="Button">
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="Button">
                                            <Border Background="{TemplateBinding Background}"
                                                    BorderBrush="{TemplateBinding BorderBrush}"
                                                    BorderThickness="{TemplateBinding BorderThickness}"
                                                    CornerRadius="4">
                                                <ContentPresenter HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"/>
                                            </Border>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                                <Style.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter Property="Background" Value="#FFB44BC2"/>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Style>
                    </Button>
                    <Button Content="Chiudi" Width="100" Height="30" Margin="5"
                            Background="#607D8B" Foreground="White"
                            BorderBrush="#607D8B" BorderThickness="1"
                            Click="ChiudiButton_Click" Cursor="Hand">
                        <Button.Style>
                            <Style TargetType="Button">
                                <Setter Property="Template">
                                    <Setter.Value>
                                        <ControlTemplate TargetType="Button">
                                            <Border Background="{TemplateBinding Background}"
                                                    BorderBrush="{TemplateBinding BorderBrush}"
                                                    BorderThickness="{TemplateBinding BorderThickness}"
                                                    CornerRadius="4">
                                                <ContentPresenter HorizontalAlignment="Center"
                                                                VerticalAlignment="Center"/>
                                            </Border>
                                        </ControlTemplate>
                                    </Setter.Value>
                                </Setter>
                                <Style.Triggers>
                                    <Trigger Property="IsMouseOver" Value="True">
                                        <Setter Property="Background" Value="#78909C"/>
                                    </Trigger>
                                </Style.Triggers>
                            </Style>
                        </Button.Style>
                    </Button>
                </StackPanel>
            </StatusBarItem>
        </StatusBar>
    </Grid>
</Window>
